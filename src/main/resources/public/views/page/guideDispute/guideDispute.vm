#extends("page/layout/mainDesHome.vm")
#block("content")
	<!-- 内容 -->
    <div class="mainHome-des-content bg-white">
   		<div class="dispute-bg">
   			<div class="wraper1200">
   				<div class="dispute-bg-s"></div>
   			</div>
   		</div>
    	<div class="wraper1200">
			<div class="type-state">
				<span class="name"></span>
				<span class="dispute-tit"><i></i>纠纷基本信息</span>
			</div>
			<div class="type-state">
				<span class="name"><em>*</em>纠纷类型：</span>
				<select class="wid598" id="_dispute">
					<option value="">请选择</option>
					<option value="1301">道交事故</option>
					<option value="1001">婚姻家庭</option>
					<option value="1202">民间借贷</option>
					<option value="1401">劳动争议</option>
					<option value="1205">物业服务合同</option>
					<option value="1999">其他民事</option>
				</select>
			</div>
			<div class="type-state">
				<span class="name">涉及金额：</span>
				<input class="wid598" type="text" name="" placeholder="输入金额（元）" id="_money">
			</div>
			<div class="type-state">
				<span class="name" style="vertical-align: top;">主要诉求：</span>
				<textarea placeholder="请输入0~500个字" id="_reason"></textarea>
			</div>
			<div class="btn-center">
				<a class="start-analysis" href="javascript:;" id="_analyse">开始分析</a>
			</div>
    	</div>
    </div>

#end
#script()
    require(['page/guideDispute/guideDispute'], function(chooseType) {
        new chooseType();
    });
#end

